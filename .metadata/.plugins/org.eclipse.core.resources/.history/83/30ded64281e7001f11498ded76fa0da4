 <!-- 어딘가에 bean을 등록해놓으면 @Autowired 해석해서 해당 객체를
 가져다 쓰겠다는 의미  -->
<bean id = "h1" class = "com.puft.feb101.human.Human">
 <property name = "name" value = "김뜻돌"/>
 <property name = "age" value = "200"/>
 <property name = "nickname">
  <list value-type = "java.lang.String">
  <value>비버</value>
  <value>돼지</value>
  <value>저스트</value>
  
  
  
  </list>
 </property>
 <property name = "friend">
 <list value-type = "java.lang.String">
 <value>곽두팔</value>
  <value>김춘삼</value>
 </list>
 
 </property>
 <property name = "family">
   <map key-type = "java.lang.String" value-type = "java.lang.Integer">
   <entry key = "아버지" value = "300"></entry>
   <entry key = "어머니" value = "290"></entry>
   </map>
 
 </property>
</bean>
 <bean id = "d1" class = "com.puft.feb101.dog.Dog">
 <property name = "name" value = "멈무"/>
 <property name = "age" value = "5"/> 
 </bean>
 <bean id = "c1" class = "com.puft.feb101.cat.Cat">
 <property name = "name" value = "먀먕"/>
 <property name = "age" value = "2"/>

 
 
 
</bean>
 
 
 <context:annotation-config>
 
 </context:annotation-config>





</beans>
