<script type = "text/javascript">
 // $(선택자).이벤트(콜백함수)
 // 콜백함수 : 어떤 이벤트에 의해서 호출되어지는 함수
/*  $(document).ready(function init(){ //이(문서).준비다되면(자동 호출될 함수)
   alert("준비 갈 완료 !");	 
 }); */
$(function(){
	alert("점심 먹으러 준비 갈 완료 !");
	
	
}) 


//CSS 선택자 이용
$(function(){
	 $("h1").click(() => {
		 
		 alert("우우우우우우우~~~");
	 });
	 $("h2").click(function(){
		alert("와아ㅏ아아"); 
	 });
	 
	 // 스위치 기능
	 
	 
	 
 });




</script>
