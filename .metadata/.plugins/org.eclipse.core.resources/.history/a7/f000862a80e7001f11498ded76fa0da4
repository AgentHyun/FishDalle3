 <!-- 어딘가에 bean을 등록해놓으면 @Autowired 해석해서 해당 객체를
 가져다 쓰겠다는 의미  -->
 <bean id = "d1" class = "com.puft.feb101.dog.Dog">
 <property name = "name" value = "멈무"/>
 <property name = "age" value = "5"/> 
 </bean>
 <bean id = "c1" class = "com.puft.feb101.cat.Cat">
 <property name = "name" value = "먀먕"/>
 <property name = "age" value = "2"/>
 
</bean>
 
 
 <context:annotation-config>
 
 </context:annotation-config>





