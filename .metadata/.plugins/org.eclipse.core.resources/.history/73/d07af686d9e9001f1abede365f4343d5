<script type = "text/javascript">

$(function(){
	
	// ㅋㅋㅋㅋㅋㅋ => 글자색 : green / 배경색 : red
	// ㅎㅎㅎㅎㅎㅎ => 글자색 : yellow/ 배경색 : blue
	// ㅠㅠㅠㅠㅠㅠ=> 글자색 : yellow/ 배경색 : teal
	
	
	  $("tr").each(function(i, h){
		  	 
	      $(h).css("color", "green");
	      $(h).css("background-color", "red");
	   
	  	 
	  	 });
	
	
	
	
	
	
	
	
});



</script>
